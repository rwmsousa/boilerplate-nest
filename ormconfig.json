{
  "type": "postgres",
  "host": "${DATABASE_HOST}",
  "port": "${DATABASE_PORT}",
  "username": "${DATABASE_USER}",
  "password": "${DATABASE_PASSWORD}",
  "database": "${DATABASE_NAME}",
  "entities": ["dist/**/*.entity{.ts,.js}"],
  "migrations": ["dist/migrations/*{.ts,.js}"],
  "cli": {
    "migrationsDir": "src/migrations"
  }
}
